<?xml version="1.0" ?>
<UserControl 
  x:Class="HisouSangokushiZero2_1_Uno.Pages.Ask"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Visibility="Collapsed">
  <StackPanel Padding="5" Background="#FDDD" HorizontalAlignment="Center" VerticalAlignment="Center">
    <TextBlock x:Name="TitleTextBlock" HorizontalAlignment="Center"/>
    <StackPanel Height="5"/>
    <ScrollViewer x:Name="ContentsView" Background="White">
      <StackPanel x:Name="ContentsPanel" VerticalAlignment="Top"/>
    </ScrollViewer>
    <StackPanel Height="5"/>
    <StackPanel Orientation="Horizontal">
      <Button x:Name="OkButton" Height="80" Background="#FEEE">
        <TextBlock x:Name="OkButtonText"/>
      </Button>
      <StackPanel Width="10"/>
      <Button x:Name="CloseButton" Height="80" Background="#FEEE">
        <TextBlock Text="閉じる"/>
      </Button>
    </StackPanel>
  </StackPanel>
</UserControl>