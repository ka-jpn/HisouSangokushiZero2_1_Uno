      EntryPoint="$targetentrypoint$">
      <uap:VisualElements  DisplayName="悲愴三国志Zero2-1" Description="悲愴三国志Zero2-1 UnoPlatform"  Square150x150Logo="Assets\Icons\icon.png" Square44x44Logo="Assets\Icons\icon.png">
